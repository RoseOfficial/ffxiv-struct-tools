{
  "name": "ffxiv-struct-tools",
  "version": "0.1.0",
  "description": "CLI tools for FFXIVClientStructs maintainers",
  "type": "module",
  "bin": {
    "fst": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "link": "bun link"
  },
  "keywords": [
    "ffxiv",
    "clientstructs",
    "reverse-engineering",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
